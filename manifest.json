{
  "name": "Reload Gnome",
  "version": "1.21",
  "description":"A kiosk mode idle-refresh utility. Reloads a home page after N seconds of inactivity.",
  "icons": { "48": "gnome-48.png",
             "36": "gnome-36.png",
            "128": "gnome-128.png" },
